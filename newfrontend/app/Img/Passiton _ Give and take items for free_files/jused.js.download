function ratemover(rid)

{

	var i;

	for(i=1;i<=5;i++)

	{

		if(i<=rid)

		{

			document.getElementById('rn'+i).style.display='none';

			document.getElementById('rs'+i).style.display='block';

		}

		else

		{

			document.getElementById('rs'+i).style.display='none';

			document.getElementById('rn'+i).style.display='block';

		}

	}

}

